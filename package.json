    "url": "git+https://github.com/smartprocure/contexture.git"
  },
  "keywords": [
    "search",
    "data-context"
  ],
  "author": "Samuel Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartprocure/contexture/issues"
  },
  "homepage": "https://github.com/smartprocure/contexture#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "futil-js": "^1.31.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "duti": "^0.7.0",
    "mocha": "^3.5.3",
    "prettier": "^1.7.2"
  }
}
